:  <toolset>msvc:<link>static ; # <link>static ;
install lib-install : Fingerprints : <location>$(RDBASE)/bin ;

exe test1 : test1.cpp Fingerprints
  ../SmilesParse//SmilesParse ../Subgraphs//Subgraphs
  ..//GraphMol ../../DataStructs//DataStructs ../../RDGeneral//RDGeneral 
  ../../Geometry//RDGeometry ../..//lapacklibs ;

exe bench : bench.cpp Fingerprints
  ../FileParsers//FileParsers ../Subgraphs//Subgraphs
  ..//GraphMol ../../DataStructs//DataStructs ../../RDGeneral//RDGeneral 
  ../../Geometry//RDGeometry ../..//lapacklibs profiler ;

lib profiler : : <name>profiler ;

install regrs : test1 bench
  : <location>testExecs ;




